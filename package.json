{
  "name": "query-face",
  "nameVar": "queryFace",
  "version": "0.0.5",
  "description": "Secure SQL query building library to get rid of static endpoints. Works best with Node.js query-face-node package.",
  "main": "lib/query-face.js",
  "module": "lib/query-face.module.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "eslint --fix src/ && rollup -c",
    "test": "jest --verbose",
    "docs": "node .jsdoc/node/before-generation.js && npm run build && eslint --fix .jsdoc/src && jsdoc -c ./.jsdoc/config.js -t ./.jsdoc/custom-template && node .jsdoc/node/after-generation.js && prettier --write ./docs/query-face/**/*.html",
    "prepublishOnly": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run docs && git add ./docs",
      "pre-push": "npm test"
    }
  },
  "keywords": [
    "query-builder",
    "orm-like",
    "sql",
    "javascript",
    "client"
  ],
  "author": "engin üstün",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.transform.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@types/jest": "^24.0.15",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^2.7.0",
    "jest": "^24.8.0",
    "jsdoc": "^3.6.3",
    "prettier": "1.17.0",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-prettier": "^0.6.0",
    "rollup-plugin-terser": "^4.0.4"
  },
  "homepage": "https://github.com/enginustun/query-face#readme",
  "bugs": {
    "url": "https://github.com/enginustun/query-face/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enginustun/query-face.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
