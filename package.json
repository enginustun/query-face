{
  "name": "query-face",
  "nameVar": "queryFace",
  "version": "0.0.0",
  "description": "front-end ORM for javascript projects.",
  "main": "lib/query-face.js",
  "module": "lib/query-face.module.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "eslint --fix src/ && rollup -c",
    "test": "jest --verbose",
    "docs": "node .jsdoc/node/before-generation.js && npm run build && eslint --fix .jsdoc/src && jsdoc -c ./.jsdoc/config.js -t ./.jsdoc/custom-template && node .jsdoc/node/after-generation.js && prettier --write ./docs/query-face/**/*.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run docs && git add ./docs",
      "pre-push": "npm test"
    }
  },
  "keywords": [],
  "author": "engin üstün",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.transform.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/preset-env": "^7.4.4",
    "@types/jest": "^24.0.11",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.1.0",
    "jest": "^24.7.1",
    "jsdoc": "^3.5.5",
    "prettier": "1.17.0",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^4.0.4"
  }
}
